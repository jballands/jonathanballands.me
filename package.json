{
	"name": "jonathanballands.me",
	"version": "3.3.1",
	"description": "Third version of my personal website",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "npm run fresh && webpack-dev-server --config webpack.config.dev.js",
		"build": "npm run fresh && webpack --config webpack.config.prod.js",
		"fresh": "find ./public -name \"*.js\" -type f -delete",
		"s3:upload": "npm run build && aws s3 sync ./public s3://jonathanballands.me --exclude \".DS_Store\" --exclude \"*.js\" --cache-control max-age=2592000 && aws s3 sync ./public s3://jonathanballands.me --exclude \"*\"  --include \"*.js\" --cache-control max-age=2592000 --content-encoding \"gzip\""
	},
